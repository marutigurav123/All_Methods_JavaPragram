class DataExample
{
	String name="Raja";
	int age=24;
	{
		name="Alen";
		age=42;
	}
	DataExample(String name,int age)
	{
		this.name=name;
		this.age=age;
	}
	public static void main(String[] args) 
	{
		DataExample d=new DataExample("raj",18);
		System.out.println(d.name);
		System.out.println(d.age);
	}
}
